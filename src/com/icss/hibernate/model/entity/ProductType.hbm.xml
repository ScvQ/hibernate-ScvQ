<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>

	<class name="com.icss.hibernate.model.entity.ProductType" table="t_product_type">
		
		
		<id name="id" column="c_id" type="java.lang.String" length="32">
			<generator class="uuid"/>
		</id>
		
		<property name="num" column="c_num" type="java.lang.String" />
		<property name="name" column="c_name" type="java.lang.String" />
		
		<set name="product" inverse="true" cascade="all">
           <key column="c_product_type_id" ></key>
           <one-to-many class="com.icss.hibernate.model.entity.Product"/>
        </set>
		
	</class>


</hibernate-mapping>