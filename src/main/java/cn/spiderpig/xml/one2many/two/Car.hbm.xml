<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.spiderpig.xml.one2many.two.Car" table="car">
		<id name="id" type="java.lang.Integer" column="id" length="32">
			<generator class="identity"></generator>
		</id>
		<property name="name" type="java.lang.String" column="name" length="12"></property>
		<many-to-one name="user" class="cn.spiderpig.xml.one2many.two.User" column="user_id"></many-to-one>
	</class>
</hibernate-mapping> 